//insertion at begining: 
#include<iostream>
using namespace std;
struct Node{
    int data;
    Node* next;
};
void insertfront(Node* &head , int value){
    Node* newNode = new Node();
    newNode -> data = value;
    newNode ->next = head;
    head = newNode;
}

void printn(Node* head){
    Node* temp = head;
    while(temp != nullptr){
        cout<<temp->data<<" ";
        temp = temp->next;
    }
    cout<<endl;
}
int main()
{
    Node* head = nullptr;

    insertfront(head , 10);
    insertfront(head , 20);
    insertfront(head , 30);

    printn(head);
}
